def localization_pods
    pod 'GoogleMaps'
end

def crash_pods
    pod 'Fabric', '~> 1.7.3'
    pod 'Crashlytics', '~> 3.10'
end

def notifications_pods
    pod 'UrbanAirship-iOS-SDK'
end

def tab_pods
    pod 'XLPagerTabStrip', '~> 7.0'
end

def popup_pods
    pod 'MTPopup', '~> 1.0.2'
end

def manager_pods
    pod 'ObjectMapper', '~> 3.3'
    pod 'IBMMobileFirstPlatformFoundation'
    pod 'JSONWebToken'
end

def help_pods
    pod 'SwiftyJSON'
end

def utils_pods
    pod 'PKHUD', '~> 4.2.3'
    pod 'SidebarOverlay', '~> 4.1'
end

def ui_pods
    pod 'lottie-ios'
end
    

def reachability_pods
    pod 'ReachabilitySwift', '~> 3.0.0'
end

def register_pods
    pod 'SwiftValidator', :git => 'https://github.com/jpotts18/SwiftValidator.git', :branch => 'master'
end

def project_pods
    # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
    pod 'Cartography', '~> 1.1'
    pod 'SkyFloatingLabelTextField', '~> 3.0'
    # Pods for MiClaro
    pod 'FontAwesome.swift', '~> 1.2.0'
    pod 'DLRadioButton', '~> 1.4.9'
    pod 'khenshin', :git => 'https://bitbucket.org/khipu/khenshin-pod.git', :tag => '1.298'
end

target 'mcaLoginiOS' do
    platform :ios, '9.0'
    use_frameworks!
    project '../../mcaLoginiOS/mcaLoginiOS.xcodeproj'
    workspace '../../mcaWorkspaceiOSCL/mcaWorkspaceiOSCL.xcworkspace'
    workspace '../../mcaWorkspaceiOSCENAM/mcaWorkspaceiOSCENAM.xcworkspace'
    project_pods
    #Autolinked frameworks
    manager_pods
    utils_pods
    reachability_pods
    notifications_pods
end

